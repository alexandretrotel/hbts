{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "format": {
      "cache": false
    },
    "db:push": {
      "interactive": true,
      "cache": false
    },
    "db:generate": {
      "interactive": true,
      "cache": false
    },
    "db:migrate": {
      "interactive": true,
      "cache": false
    },
    "db:studio": {
      "persistent": true,
      "cache": false
    }
  }
}
